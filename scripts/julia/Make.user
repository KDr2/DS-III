# packages in deps/srccache but not in deps/patches:
USE_SYSTEM_PCRE=1
USE_SYSTEM_LAPACK=1
USE_SYSTEM_BLAS=1
USE_SYSTEM_MBEDTLS=1
USE_SYSTEM_SUITESPARSE=1

# system LLVM is not supported
USE_SYSTEM_LLVM=0
# LLVM_CONFIG=/usr/bin/llvm-config-3.9
# LLVM_CONFIG=/usr/bin/llvm-config-4.0
# LLVM_CONFIG=/usr/bin/llvm-config-5.0

override CC = gcc-5
override CXX = g++-5
# override CXX=clang++
# CFLAGS += -g
# CFLAGS += -DGCTIME
# CFLAGS += -DGC_FINAL_STATS
# CFLAGS += -DMEMPROFILE
# CFLAGS += -DOBJPROFILE
# CXXFLAGS += -g
